<%provide(:title, "Home")%>
<div class="center jumbotron">
  <h1>Welcome to the sample app</h1>
  <h2>This is the home page for my website</h2>
  <% if logged_in? %>
    <%= link_to "Profile", current_user, class: "btn btn-lg btn-primary" %>
  <% else %>
    <%= link_to "Sign up", signup_path, class: "btn btn-lg btn-primary" %>
  <% end %>
</div>

<%= link_to image_tag('kitten.jpg', alt: "Rails logo"), '#'%>
